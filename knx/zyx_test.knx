($sequence
    ($import! (toWat) (get-module #./zyxToWasm.knx))
    ($define! expected ($quote 
        (func $add (export "add") (param $x i32) (param $y i32) (result i32)
            (i32.add
                (local.get $x)
                (local.get $y))
        )
    ))
    ($define! actual (toWat ($quote
        (func add ((x i32) (y i32)) i32
            (+ x y)))))
    
    (=? expected actual)
)
